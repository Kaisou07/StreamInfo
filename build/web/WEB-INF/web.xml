<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Conexao</servlet-name>
        <servlet-class>Conexao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Conn</servlet-name>
        <servlet-class>Conn.Conn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listarClientes</servlet-name>
        <servlet-class>listarClientes.listarClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dados</servlet-name>
        <servlet-class>Model.Dados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadastro</servlet-name>
        <servlet-class>cadastro.cadastro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>alterar</servlet-name>
        <servlet-class>alterar.alterar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>excluir</servlet-name>
        <servlet-class>excluir.excluir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarFilmes</servlet-name>
        <servlet-class>ListarFilmes.ListarFilmes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>desc_filme</servlet-name>
        <servlet-class>desc_filme.desc_filme</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Conexao</servlet-name>
        <url-pattern>/Conexao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Conn</servlet-name>
        <url-pattern>/Conn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listarClientes</servlet-name>
        <url-pattern>/listarClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dados</servlet-name>
        <url-pattern>/Dados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadastro</servlet-name>
        <url-pattern>/cadastro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>alterar</servlet-name>
        <url-pattern>/alterar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>excluir</servlet-name>
        <url-pattern>/excluir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarFilmes</servlet-name>
        <url-pattern>/ListarFilmes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>desc_filme</servlet-name>
        <url-pattern>/desc_filme</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
